{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "buildEngine",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/engine/engine.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "buildApi",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/api/api.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
  ]
}